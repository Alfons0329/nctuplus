<div class="navbar navbar-invers navbar-default" role="navigation" style="overflow-y:auto;height:100%;min-height:100%;">
	<div class="navbar-header">
		<button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".sidebar-navbar-collapse">
		<span class="sr-only">Toggle navigation</span>
		<span class="icon-bar"></span>
		<span class="icon-bar"></span>
		<span class="icon-bar"></span>
		</button>
		<span class="visible-xs navbar-brand">Sidebar menu</span>
	</div>
<div class="navbar-collapse collapse sidebar-navbar-collapse">
	<ul class="nav navbar-nav sidebar-ul" id="sidenav01">
		<% College.includes(:departments).where("id NOT IN (8,10)").each do |college| %>
			<li class="sidebar-li" >
				<a href="javascript:void(0);" data-toggle="collapse" data-target="#college-<%=college.id%>" data-parent="#sidenav01" class="collapsed">
					<%=college.name%>
				</a>
				<div class="collapse" id="college-<%=college.id%>" style="height: 0px;">
					<ul class="nav nav-list">
						
						<% college.departments.each do |dept| %>
							<li id="dept-<%=dept.id%>" style="padding-left :20px;">
								<a href="?dept_id=<%="#{dept.id}&sem_id=13"%>"><%=dept.ch_name%></a>
							</li>
						<% end %>
					</ul>
				</div>
			</li>
		<% end %>
	</ul>
</div><!--/.nav-collapse -->
