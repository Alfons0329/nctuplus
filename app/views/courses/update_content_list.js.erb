alert(1);

$("#content_list_existed_<%=j params[:old_id].to_s%>").children().each(function(index){
	if($(this).css('display')=='none')
		$(this).show();
	else
		$(this).remove() ;	
});
$("#content_list_existed_<%=j params[:old_id].to_s%>").find('td').first().html('<%=j content_type_to_html(@list.content_type) %>');
$("#content_list_existed_<%=j params[:old_id].to_s%>").find('td').second().find('span').first().html('<%=j @list.content%>');
$("#content_list_existed_<%=j params[:old_id].to_s%>").find('td').second().find('span').second().html("<%=j @list.updated_at.strftime('%Y/%m/%d %H:%M')%> <%=j @list.user.name%>");
$('#toggle-msg').addClass('text-color-green').html('更新成功').show().fadeOut(4000);
